{
  "name": "unit-manip",
  "version": "1.0.8",
  "description": "Gestion des unit√©s et grandeurs",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "node --version && mocha --exit --recursive test --require babel-register",
    "build": "rimraf dist && yarn run test && babel src -d dist",
    "prepublishOnly": "npm version patch && npm run build",
    "npmpublish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlueForestTrees/trees-common.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BlueForestTrees/trees-common/issues"
  },
  "homepage": "https://github.com/BlueForestTrees/trees-common#readme",
  "dependencies": {
    "fraction.js": "4.0.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "chai": "4.1.2",
    "mocha": "5.0.3",
    "rimraf": "2.6.2"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ]
  }
}
